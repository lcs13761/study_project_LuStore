{
    "name": "lcs/project",
    "type": "project",
    "description": ".",
    "keywords": [
        ""
    ],
    "license": "MIT",
    "require": {
        "php": "^8.0",
        "league/oauth2-google": "^4.0",
        "league/plates": "^3.4",
        "lulco/phoenix": "^2.3",
        "pecee/simple-router": "5.3.0.3",
        "phpmailer/phpmailer": "^6.5",
        "vlucas/phpdotenv": "^5.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^9"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/",
            "Bootstrap\\Database\\": "bootstrap/database/"
        },
        "files": [
            "app/Helpers/Helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "platform": {
            "php": "8.1"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}